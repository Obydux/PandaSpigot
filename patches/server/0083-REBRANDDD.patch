     private WatchdogThread(long timeoutTime, boolean restart)
     {
-        super( "PaperSpigot Watchdog Thread" );
+        super( "PandaSpigot Watchdog Thread" ); // PandaSpigot
         this.timeoutTime = timeoutTime;
         this.restart = restart;
